{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/xmatheus/muhna-api-ts.git",
  "author": "xmatheus <matheuscorreia559@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean:build && tsc",
    "clean:build": "shx rm -rf ./build",
    "start": "npm run clean:build && npm run build && node ./build/src/",
    "start:prod": "node ./build/src",
    "start:migrate:prod": "prisma migrate deploy && npm run start:prod",
    "dev": "prisma migrate dev && npm run swagger-autogen && ts-node-dev --exit-child -r dotenv/config src/index",
    "test": "jest",
    "test:e2e": "jest --config jest-e2e.config.ts",
    "prepare": "husky install",
    "prettier:write": "prettier --write .",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix",
    "swagger-autogen": "ts-node swagger.ts"
  },
  "devDependencies": {
    "@swc/core": "^1.3.89",
    "@swc/jest": "^0.2.29",
    "@types/chai": "^4.3.3",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.5.5",
    "@types/mocha": "^10.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.14.1",
    "@types/pg": "^8.10.3",
    "@types/supertest": "^2.0.13",
    "@types/swagger-ui-express": "^4.1.4",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "and": "^0.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^13.1.2",
    "mocha": "^10.1.0",
    "pg": "^8.11.3",
    "prettier": "2.8.4",
    "prisma": "^5.3.1",
    "shx": "^0.3.4",
    "supertest": "^6.3.3",
    "swagger-autogen": "^2.23.6",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@prisma/client": "^5.3.1",
    "express": "^4.18.2",
    "morgan": "^1.10.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.ts": "eslint ./"
  }
}
